{{#diff}}
  <div class="object-history__row--attributes flex-box">
    <div class="flex-size-1 object-history__attr">
      {{attrName}}
    </div>
    <div class="flex-size-1 object-history__attr">
      {{#currentVal}}
        <div>
          {{#isTextLarge .}}
            <read-more isLazyRender:from="isLazyRender" text:from="."></read-more>
          {{else}}
            {{{.}}}
          {{/isTextLarge}}
        </div>
      {{/currentVal}}
    </div>
    <div class="flex-size-1 object-history__attr">
      {{#modifiedVal}}
        <div>
          {{#isTextLarge .}}
            <read-more isLazyRender:from="isLazyRender" text:from="."></read-more>
          {{else}}
            {{{.}}}
          {{/isTextLarge}}
        </div>
      {{/modifiedVal}}
    </div>
  </div>
{{/diff}}
