set -o nounset
set -o errexit

AUDIT_ID=${1:-}

SCRIPTPATH=$( cd "$(dirname "$0")" ; pwd -P )
cd "${SCRIPTPATH}"

python -c "\
import move_snapshots
move_snapshots.move_snapshots_to_asmnt($AUDIT_ID)
"
