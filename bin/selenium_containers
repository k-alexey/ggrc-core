PROJECT="selenium"
DOCKER_COMPOSE_FILE="docker-compose-selenium.yml"

cd "$(dirname $0)/.."
source bin/containers_functions.sh

# If containers take lots of CPU, stopping them frees up CPU for building
# containers.
teardown $PROJECT

setup $PROJECT selenium

setup_for_selenium $PROJECT
